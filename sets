void main(){
 
  /*
  Sets are unordered collection of unique items.
  listing some elements in a set below...
  */

  var elements = {'O','K','Ni','Rd'};
  print(elements);
  
  //adding one element to the set
  elements.add('F');
  print(elements);
  
  //adding various elements to the set
  elements.addAll(['H','C','Ar','B']);
  print(elements);

}
