void main(){
  var name = 'Francis';
  var wrongName = 'Wrong';
  var storedName = 'Francis';
  
  //first letter of the String var
  print(wrongName[0]);
  
  if(name == storedName){
    print("Name matched stored name!");
    print("Welcome to the app!");
  } else{
    print("Name provided did not match stored name!");
  }
}
