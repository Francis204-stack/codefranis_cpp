void main() {
  //below are the years spent by employees working for Francel Enterprise
  var employees = {'David': 10, 'Stephen': 7, 'Selasie': 3};
  print(employees);

  print(employees['David']);

  //including another employee to the stated list above
  employees['Lois'] = 8;
  print(employees);

  //removing an employee from the list and assigning its value to another variable
  var removedValue = employees.remove('Stephen');
  print (removedValue);
  print (employees);
}
