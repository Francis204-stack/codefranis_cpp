void main(){
  var myListNum = [10,20,30];
  var myListAlphaOne = ['A','B','C','D'];
  var myListAlphaTwo = ['X','Y','Z'];
  var myMixedList = [10,20,'F'];
  
  print(myListNum);
  print(myListAlphaOne);
  print(myListAlphaTwo);
  print(myMixedList);
  
  //displaying the first component of a variable
  print(myListNum[0]);
  
  //displaying a subset of a variable
  print(myListAlphaOne.sublist(0,3));
  
  //displaying the length of the list in a variable
  print(myMixedList.length);
  
  /*
  combining two lists together
  NB:you can only combine lists of the same data type
  */
  print(myListAlphaOne + myListAlphaTwo);
}
